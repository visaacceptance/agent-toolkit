{
  "compilerOptions": {
    "target": "ES2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true

  },
  "include": [
    "*.ts",
    "src/**/*",
    "src/**/*.ts",
    "types/**/*.d.ts"
  ],
  "references": [

  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}