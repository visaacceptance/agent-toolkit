{
  "name": "@visaacceptance/agent-toolkit",
  "version": "0.0.93",
  "description": "This toolkit provides a TypeScript implementation for interacting with Visa Acceptance APIs.",
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "tsup",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn src/index.ts",
    "lint": "eslint . --ext .ts",
    "prepare": "npm run build",
    "test": "jest"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./ai-sdk": {
      "types": "./ai-sdk/index.d.ts",
      "require": "./ai-sdk/index.js",
      "import": "./ai-sdk/index.mjs"
    },
    "./modelcontextprotocol": {
      "types": "./modelcontextprotocol/index.d.ts",
      "require": "./modelcontextprotocol/index.js",
      "import": "./modelcontextprotocol/index.mjs"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "ai-sdk/**/*",
    "modelcontextprotocol/**/*",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "ai": "^4.3.11",
    "axios": "^1.9.0",
    "cybersource-rest-client": "^0.0.67",
    "dotenv": "^16.5.0",
    "openai": "^4.96.2",
    "superagent": "^10.2.0",
    "zod": "^3.24.3"
  }
}